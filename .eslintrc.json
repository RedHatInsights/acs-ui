{
    "env": {
        "browser": true,
        "es2021": true
    },
    "globals": {
        "insights": "readonly",
        "shallow": "readonly",
        "render": "readonly",
        "mount": "readonly"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "@redhat-cloud-services/eslint-config-redhat-cloud-services"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "./config/dev.webpack.config.js"
            }
        }
    }
}
